{
  "name": "@cdssnc/logger",
  "version": "0.0.26",
  "description": "CDS logger",
  "main": "dist/logger.js",
  "umd:main": "dist/logger.umd.js",
  "module": "dist/index.mjs",
  "source": "src/index.mjs",
  "unpkg": "dist/logger.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "microbundle",
    "build": "microbundle --no-compress",
    "dev": "microbundle watch",
    "lint": "eslint src/*.js",
    "test": "jest",
    "release": "release-it"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && lint-staged",
      "pre-push": "npm test"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.2.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.3.1",
    "jest": "^24.0.0",
    "lint-staged": "^8.1.1",
    "microbundle": "^0.9.0",
    "prettier": "^1.16.2",
    "release-it": "^10.0.7"
  },
  "dependencies": {
    "uuid": "^3.3.2"
  }
}
